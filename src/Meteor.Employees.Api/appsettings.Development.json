{
  "FeatureManagement": {
    "RunMigrationsOnStartup": true
  },
  "Security": {
    "Hashing": {
      "IterationsCount": 10000,
      "DefaultSaltLenght": 8,
      "RequestBytesLength": 16
    }
  },
  "Routing": {
    "ControllerUrl": "http://localhost:80"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Meteor.Employees.Api.HostedServices.MigrationsRunner": "Information",
      "Meteor.Employees.Api.Interceptors.ExceptionsHandlingInterceptor": "Information"
    }
  }
}
